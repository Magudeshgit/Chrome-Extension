{
    "name": "BrowseWind",
    "description": "Restore your browsing tabs across devices",
    "version": "1.0",
    "manifest_version": 3,
    "action":
    {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "offscreen"
    ]
    
}