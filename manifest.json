{
    "name": "BrowseWind",
    "description": "Restore your browsing tabs across devices",
    "version": "1.0",
    "manifest_version": 3,
    "action":
    {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "offscreen"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
    }
    
}